{
  "name": "classify",
  "version": "1.0.0",
  "description": "Classify web interface",
  "main": "electron.js",
  "scripts": {
    "build": "tsc && rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "old-start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "start": "tsc && concurrently \"npm run tsc:w\" \"webpack-dev-server --inline --progress --port 3000\"",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install",
    "electron": "electron ."
  },
